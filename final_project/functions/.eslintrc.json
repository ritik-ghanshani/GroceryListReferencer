{
  "extends": ["prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
      "prettier/prettier": ["error", { "endOfLine": "auto" }]
  },
  "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "env": {
      "es6": true,
      "browser": true,
      "node": true
  }
}
